{
  "expo": {
    "name": "group-decision-making-app",
    "slug": "group-decision-making-app",
    "scheme": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cooperation.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cooperation.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cooperation.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.amujueee.groupdecisionmakingapp"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/cooperation.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7de7a690-1f10-4c9e-ac6e-159a4e343db8"
      }
    }
  }
}
